{
  "name": "csgo-checker",
  "version": "1.2.1",
  "description": "csgo-checker",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win",
    "publish": "electron-builder --win --publish always"
  },
  "author": "dumbasPL",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "csgo-friendcode": "^3.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "electron-updater": "^4.3.9",
    "fast-deep-equal": "^3.1.3",
    "got": "^11.8.2",
    "pbkdf2": "^3.1.2",
    "protobufjs": "^6.11.3",
    "showdown": "^1.9.0",
    "simple-json-db": "^1.2.3",
    "steam-totp": "^2.1.1",
    "steam-user": "^4.20.2"
  },
  "devDependencies": {
    "electron": "^16.2.6",
    "electron-builder": "^22.13.1"
  },
  "build": {
    "appId": "cc.nezu.csgochecker",
    "productName": "CSGO account checker",
    "win": {
      "artifactName": "CSGO Checker installer.${ext}",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "portable": {
      "artifactName": "csgo-checker_portable.exe"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "dumbasPL",
        "repo": "csgo-checker"
      }
    ]
  }
}
